  "html": "<!DOCTYPE html><html lang=\"en-us\"><head>  <meta charset=\"utf-8\">  <title>Dapr - Distributed Application Runtime</title>  <!-- mobile responsive meta -->  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=5\">  <meta name=\"description\" content=\"APIs for building secure and reliable microservices\">  <meta name=\"keywords\" content=\"dapr, microservices, distributed applications, runtime, kubernetes\">  <meta name=\"author\" content=\"Dapr Maintainers\">  <meta name=\"generator\" content=\"Hugo 0.74.3\" />  <!-- plugins -->    <link rel=\"stylesheet\" href=\"https://dapr.io/plugins/themify-icons/themify-icons.css\">    <link rel=\"stylesheet\" href=\"https://dapr.io/plugins/slick/slick.css\">    <!-- Main Stylesheet -->    <link href=\"https://dapr.io/scss/style.min.css\" rel=\"stylesheet\" media=\"screen\"/>  <!-- Font Awesome -->  <link rel=\"stylesheet\" href=\"https://use.fontawesome.com/releases/v5.0.7/css/all.css\"/>  <!-- Google Font CDN -->  <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\" />  <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin />  <link href=\"https://fonts.googleapis.com/css2?family=Noto+Sans:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap\" rel=\"stylesheet\">  <!--Favicon-->  <link rel=\"shortcut icon\" href=\"https://dapr.io/images/favicon.ico\" type=\"image/x-icon\">  <link rel=\"icon\" href=\"https://dapr.io/images/favicon.png\" type=\"image/x-icon\">    <meta property=\"og:title\" content=\"Dapr - Distributed Application Runtime\" /><meta property=\"og:description\" content=\"APIs for building secure and reliable microservices\" /><meta property=\"og:type\" content=\"website\" /><meta property=\"og:url\" content=\"https://dapr.io/\" />    <script type=\"application/javascript\">var doNotTrack = false;if (!doNotTrack) {window.ga=window.ga||function(){(ga.q=ga.q||[]).pus