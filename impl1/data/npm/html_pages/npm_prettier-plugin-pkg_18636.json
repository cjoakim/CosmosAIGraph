  "html": "{\"payload\":{\"allShortcutsEnabled\":false,\"path\":\"packages/pkg\",\"repo\":{\"id\":209946816,\"defaultBranch\":\"master\",\"name\":\"prettier\",\"ownerLogin\":\"un-ts\",\"currentUserCanPush\":false,\"isFork\":false,\"isEmpty\":false,\"createdAt\":\"2019-09-21T07:57:37.000Z\",\"ownerAvatar\":\"https://avatars.githubusercontent.com/u/106909066?v=4\",\"public\":true,\"private\":false,\"isOrgOwned\":true},\"currentUser\":null,\"refInfo\":{\"name\":\"master\",\"listCacheKey\":\"v0:1709267539.0\",\"canEdit\":false,\"refType\":\"branch\",\"currentOid\":\"bff7578d858ffaf145fb3e497c200c7bfdb6158c\"},\"tree\":{\"items\":[{\"name\":\"src\",\"path\":\"packages/pkg/src\",\"contentType\":\"directory\"},{\"name\":\"test\",\"path\":\"packages/pkg/test\",\"contentType\":\"directory\"},{\"name\":\"CHANGELOG.md\",\"path\":\"packages/pkg/CHANGELOG.md\",\"contentType\":\"file\"},{\"name\":\"LICENSE\",\"path\":\"packages/pkg/LICENSE\",\"contentType\":\"file\"},{\"name\":\"README.md\",\"path\":\"packages/pkg/README.md\",\"contentType\":\"file\"},{\"name\":\"package.json\",\"path\":\"packages/pkg/package.json\",\"contentType\":\"file\"},{\"name\":\"tsconfig.json\",\"path\":\"packages/pkg/tsconfig.json\",\"contentType\":\"file\"}],\"templateDirectorySuggestionUrl\":null,\"readme\":{\"displayName\":\"README.md\",\"richText\":\"<article class=\\\"markdown-body entry-content container-lg\\\" itemprop=\\\"text\\\"><p dir=\\\"auto\\\"><a target=\\\"_blank\\\" rel=\\\"noopener noreferrer nofollow\\\" href=\\\"https://raw.githubusercontent.com/un-ts/prettier/master/assets/pkg.svg?sanitize=true\\\"><img src=\\\"https://raw.githubusercontent.com/un-ts/prettier/master/assets/pkg.svg?sanitize=true\\\" alt=\\\"banner\\\" style=\\\"max-width: 100%;\\\"></a></p>\\n<div class=\\\"markdown-heading\\\" dir=\\\"auto\\\"><h1 tabindex=\\\"-1\\\" class=\\\"heading-element\\\" dir=\\\"auto\\\">prettier-plugin-pkg <a target=\\\"_blank\\\" rel=\\\"noopener noreferrer nofollow\\\" href=\\\"https://ca