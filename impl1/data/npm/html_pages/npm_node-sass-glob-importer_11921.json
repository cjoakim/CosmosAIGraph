er: 'sass-loader',\\n            options: {\\n              sassOptions: {\\n                importer: globImporter()\\n              }\\n            }\\n          }\\n        ]\\n      }\\n    ]\\n  },\\n  plugins: [\\n    new MiniCssExtractPlugin({\\n      filename: 'style.css'\\n    })\\n  ]\\n}\\\"><pre><span class=\\\"pl-c\\\">// webpack.config.js</span>\\n<span class=\\\"pl-k\\\">const</span> <span class=\\\"pl-s1\\\">globImporter</span> <span class=\\\"pl-c1\\\">=</span> <span class=\\\"pl-en\\\">require</span><span class=\\\"pl-kos\\\">(</span><span class=\\\"pl-s\\\">'node-sass-glob-importer'</span><span class=\\\"pl-kos\\\">)</span><span class=\\\"pl-kos\\\">;</span>\\n<span class=\\\"pl-k\\\">const</span> <span class=\\\"pl-v\\\">MiniCssExtractPlugin</span> <span class=\\\"pl-c1\\\">=</span> <span class=\\\"pl-en\\\">require</span><span class=\\\"pl-kos\\\">(</span><span class=\\\"pl-s\\\">'mini-css-extract-plugin'</span><span class=\\\"pl-kos\\\">)</span><span class=\\\"pl-kos\\\">;</span>\\n\\n<span class=\\\"pl-smi\\\">module</span><span class=\\\"pl-kos\\\">.</span><span class=\\\"pl-c1\\\">exports</span> <span class=\\\"pl-c1\\\">=</span> <span class=\\\"pl-kos\\\">{</span>\\n  <span class=\\\"pl-c1\\\">module</span>: <span class=\\\"pl-kos\\\">{</span>\\n    <span class=\\\"pl-c1\\\">rules</span>: <span class=\\\"pl-kos\\\">[</span>\\n      <span class=\\\"pl-kos\\\">{</span>\\n        <span class=\\\"pl-c1\\\">test</span>: <span class=\\\"pl-pds\\\"><span class=\\\"pl-c1\\\">/</span><span class=\\\"pl-cce\\\">\\\\.</span>scss<span class=\\\"pl-cce\\\">$</span><span class=\\\"pl-c1\\\">/</span></span><span class=\\\"pl-kos\\\">,</span>\\n        <span class=\\\"pl-c1\\\">use</span>: <span class=\\\"pl-kos\\\">[</span>\\n          <span class=\\\"pl-kos\\\">{</span>\\n            <span class=\\\"pl-c1\\\">loader</span>: <span class=\\\"pl-v\\\">MiniCssExtractPlugin</span><span class=\\\"pl-kos\\\">.</span><span class=\\\"pl-c1\\\">loader</span><span class=\\\"pl-kos\\\">,</span>\\n          <span class=\\\"pl-kos\\\">}</span><span class=\\\"pl-kos\\\">,</span>\\n          <span class=\\\"pl-kos\\\">{</span>\\n            <span class=\\\"pl-c1\\\">loader</span>: <span class=\\\"pl-s\\\">'css-loader'</span>\\n          <span class=\\\"pl-kos\\\">}</span><span class=\\\"pl-kos\\\">,</span> <span class=\\\"pl-kos\\\">{</span>\\n            <span class=\\\"pl-c1\\\">loader</span>: <span class=\\\"pl-s\\\">'sass-loader'</span><span class=\\\"pl-kos\\\">,</span>\\n            <span class=\\\"pl-c1\\\">options</span>: <span class=\\\"pl-kos\\\">{</span>\\n              <span class=\\\"pl-c1\\\">sassOptions</span>: <span class=\\\"pl-kos\\\">{</span>\\n                <span class=\\\"pl-c1\\\">importer</span>: <span class=\\\"pl-s1\\\">globImporter</span><span class=\\\"pl-kos\\\">(</span><span class=\\\"pl-kos\\\">)</span>\\n              <span class=\\\"pl-kos\\\">}</span>\\n            <span class=\\\"pl-kos\\\">}</span>\\n          <span class=\\\"pl-kos\\\">}</span>\\n        <span class=\\\"pl-kos\\\">]</span>\\n      <span class=\\\"pl-kos\\\">}</span>\\n    <span class=\\\"pl-kos\\\">]</span>\\n  <span class=\\\"pl-kos\\\">}</span><span class=\\\"pl-kos\\\">,</span>\\n  <span class=\\\"pl-c1\\\">plugins</span>: <span class=\\\"pl-kos\\\">[</span>\\n    <span class=\\\"pl-k\\\">new</span> <span class=\\\"pl-v\\\">MiniCssExtractPlugin</span><span class=\\\"pl-kos\\\">(</span><span class=\\\"pl-kos\\\">{</span>\\n      <span class=\\\"pl-c1\\\">filename</span>: <span class=\\\"pl-s\\\">'style.css'</span>\\n    <span class=\\\"pl-kos\\\">}</span><span class=\\\"pl-kos\\\">)</span>\\n  <span class=\\\"pl-kos\\\">]</span>\\n<span class=\\\"pl-kos\\\">}</span></pre></div>\\n<div class=\\\"markdown-heading\\\" dir=\\\"auto\\\"><h3 tabindex=\\\"-1\\\" class=\\\"heading-element\\\" dir=\\\"auto\\\">CLI</h3><a id=\\\"user-content-cli\\\" class=\\\"anchor-element\\\" aria-label=\\\"Permalink: CLI\\\" href=\\\"#cli\\\"><svg 